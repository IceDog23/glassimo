{% capture listItems %}
                        {%- paginate collection.products by 24 -%}
                        {% for item in collection.products %}
                        <div role="listitem" class="product-item w-dyn-item">
                <div class="add-to-cart">
                  {%- form "product", item, data-node-type: "commerce-add-to-cart-form", class: "w-commerce-commerceaddtocartform" -%}{%- unless item.has_only_default_variant or product.variants.size == 1 -%}<div data-node-type="add-to-cart-option-list" position-id="46ba3cb6-2d2f-7a99-dced-7a9f2e6d54b5" template-part-bind="46ba3cb6-2d2f-7a99-dced-7a9f2e6d54b5">{%- for option in item.options_with_values -%}<fieldset data-node-type="add-to-cart-option" template-part-bind="46ba3cb6-2d2f-7a99-dced-7a9f2e6d54b6" style="{% if item.has_only_default_variant or product.variants.size == 1 %}display: none; {% endif %}"><label data-node-type="add-to-cart-option-label" template-part-bind="46ba3cb6-2d2f-7a99-dced-7a9f2e6d54b7"></label><select data-node-type="add-to-cart-option-select" class="w-select" style="display: none;" name="{{ "option" | append: forloop.index }}">{%- render "product-options", option: option -%}</select><div data-node-type="add-to-cart-option-pill-group" class="button-group-2 w-commerce-commerceaddtocartoptionpillgroup" aria-label="{{ "option" | append: forloop.index  }}">{%- for value in option.values -%}<div data-node-type="add-to-cart-option-pill" class="color-button w-commerce-commerceaddtocartoptionpill" data-option-name="{{ value | handle }}"><div data-node-type="block" class="text-block-5" fs-cmsfilter-field="barva" template-part-bind="46ba3cb6-2d2f-7a99-dced-7a9f2e6d54bb">{{ value }}</div></div>{%- endfor -%}</div></fieldset>{%- endfor -%}</div>{%- endunless -%}{%- render "product-variants-data-currency" product: item -%}{%- endform -%}
                </div>
                <div class="div-block-15">
                  <div fs-cmsfilter-field="dostupnost" class="paragraph skladem">{{ item.metafields.udesly['dostupnost-3'] }}</div>
                </div>
                <a href="{{ item.url }}" class="link-block w-inline-block">
                  <div class="overlay-on-product-2"></div><img alt="{{ item.featured_image.alt }}" src="{{ item.featured_image | img_url: 'master' }}" height="254" class="product-photo" data-commerce-type="variation-image" srcset="{{ item.featured_image | img_url: '1920x' }} 1920w,{{ item.featured_image | img_url: '1600x' }} 1600w,{{ item.featured_image | img_url: '1366x' }} 1366w,{{ item.featured_image | img_url: '1024x' }} 1024w,{{ item.featured_image | img_url: '768x' }} 768w,{{ item.featured_image | img_url: '640x' }} 640w">
                  <div class="product-text-fields">
                    <div class="product-namexdesigner-wrapper">
                      {%- assign refItem = item.metafields.custom.designer.value -%}<div fs-cmsfilter-field="designer" class="designer-name">{{ refItem.jmeno }}</div>
                      <div fs-cmssort-field="nazev" class="product-name">{{ item.title }}</div>
                    </div>
                    <div data-commerce-type="variation-price" fs-cmssort-field="cena" fs-cmsfilter-field="cena" class="price">{{ item.price | money_with_currency }}</div>
                  </div>
                </a>
                <div fs-cmssort-type="date" class="date">{{ item.published_at | date: "%B %e, %Y" }}</div>
              </div>
                        {% endfor %}
                        {% capture pagination %}
                        {% render 'pagination', paginate: paginate %}
                      {% endcapture %} 
                        {%- endpaginate -%}
                        {% endcapture %}
{% assign items_length = listItems | strip | size %}
    <div id="w-node-ab57290d-3cc7-f10d-2fa7-7143d7cbcbff-661962f9" class="product-list-wrapper w-dyn-list" udy-collection="product">
            {%- unless items_length == 0 -%}<div fs-cmsload-mode="pagination" class="product-list w-dyn-items" fs-cmsfilter-element="list" role="list" fs-cmssort-element="list" fs-cmsload-pagesiblings="2" fs-cmsload-element="list" fs-cmsfilter-showquery="true">
              {{ listItems }}
            </div>
            {{ pagination }}
          {%- endunless -%}
            {%- if items_length == 0 -%}<div class="w-dyn-empty">
              <div>No items found.</div>
            </div>{%- endif -%}
