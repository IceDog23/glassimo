{% capture listItems %}
                        {%- paginate collection.products by 24 -%}
                        {% for item in collection.products %}
                        <div role="listitem" class="product-item w-dyn-item">
                <div class="div-block-15">
                  <div fs-cmsfilter-field="dostupnost" class="paragraph skladem">This is some text inside of a div block.</div>
                </div>
                <a href="#" class="link-block w-inline-block">
                  <div class="overlay-on-product-2"></div><img alt="" src="https://d3e54v103j8qbb.cloudfront.net/plugins/Basic/assets/placeholder.60f9b1840c.svg" height="254" class="product-photo"><img alt="{{ item.metafields.custom['image'].value.alt }}" loading="lazy" src="{{ item.metafields.custom['image'].value | img_url: 'master' }}" class="variant-photo">
                  <div class="product-text-fields">
                    <div class="product-namexdesigner-wrapper">
                      <div fs-cmsfilter-field="designer" class="designer-name">This is some text inside of a div block.</div>
                      <div fs-cmssort-field="nazev" class="product-name">This is some text inside of a div block.</div>
                    </div>
                    <div fs-cmssort-field="cena" class="price">This is some text inside of a div block.</div>
                  </div>
                </a>
                <div fs-cmssort-type="date" class="date">This is some text inside of a div block.</div>
              </div>
                        {% endfor %}
                        {% capture pagination %}
                        {% render 'pagination', paginate: paginate %}
                      {% endcapture %} 
                        {%- endpaginate -%}
                        {% endcapture %}
{% assign items_length = listItems | strip | size %}
    <div id="w-node-_2383dea9-cf2f-9423-1275-d1e82dd72af9-8fc70bb7" class="product-list-wrapper w-dyn-list" udy-collection="product">
            {%- unless items_length == 0 -%}<div fs-cmsload-mode="pagination" class="product-list w-dyn-items" fs-cmsfilter-element="list" role="list" fs-cmssort-element="list" fs-cmsload-pagesiblings="2" fs-cmsload-element="list" fs-cmsfilter-showquery="true">
              {{ listItems }}
            </div>
            {{ pagination }}
          {%- endunless -%}
            {%- if items_length == 0 -%}<div class="w-dyn-empty">
              <div>No items found.</div>
            </div>{%- endif -%}
