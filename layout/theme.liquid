<!DOCTYPE html>
<!-- Last Published: {{ "now" | date: "%Y-%m-%dT%H:%M:%S.%L%z" }} -->
<html data-wf-page="{{ wfPage }}" data-wf-site="65397d9f3766491d0cad2172" lang="{{ request.locale.iso_code }}" data-money-format="{{ shop.money_with_currency_format | escape }}"><head>{%- liquid
    if headerSnippetPath
        include headerSnippetPath
    else
        render "__default-head"
    endif
-%}
{{content_for_header}}
{% if template contains 'customers' %}
    {{ "shopify_common.js" | shopify_asset_url | script_tag }}
{% endif %}
</head>
<body class="{{ bodyClasses }} {{ "template-" | append: request.page_type | handle }}">
{% if headerSnippetPath %}
    {{ content_for_layout }}
{% else %}
    {% include "backup-layout" %}
{% endif %}
</body></html>